@model List<OnlineShop.Models.EF.Menu>
<div class="menu">
    <ul>
        @for (int i = 0; i < Model.Count; i++)
        {
            <li>
                <a title="@Model[i].Text" href="@Model[i].Link" target="@Model[i].Target">@Model[i].Text</a>
            </li>
        }
        <div class="clear"></div>
    </ul>
</div>